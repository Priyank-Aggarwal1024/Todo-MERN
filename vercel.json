{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/task",
            "dest": "index.js"
        },
        {
            "src": "/api/task/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "build/"
        }
    ]
}